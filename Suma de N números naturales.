//Suma de N Números naturales
//Autor: Braian Adair Maldonado Luna
//Creado: 23/10/2017 


#include <iostream>
using namespace std;
int main () {
    int N=0, sum=0;
    cout<<"Dame un número natural"<<endl;
    cin>>N;
    for (int i=0; i<=N, i++;){
    sum=sum+i;}
    cout<<"La suma de los N números naturales es: "<<sum<<endl;
    return 0;
}
