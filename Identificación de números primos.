//Identificación números primos
//Autor: Braian Adair Maldonado Luna
//Creado: 23/10/2017 


#include <iostream>
using namespace std;
int main () {
        int N=0;
        int prime=0;
    cout<<"Dame un número entero"<<endl;
    cin>>N;
    
    for(int i=2; i<=(N/2); i++)
    {
        if ((N%i)==0){
            prime=1;
            break;
                    }
    }
        if (prime==0)
            cout<<"Tú número es primo"<<endl;
            else cout<<"Tú número no es primo"<<endl;
return 0;
            }
